<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.ietr.dftools.workflow.tasks">
      <task
            id="fi.abo.preesm.dataparallel.DataParallel"
            name="Data-Parallel"
            type="fi.abo.preesm.dataparallel.DataParallel">
         <inputs>
            <input
                  id="SDF"
                  object="org.ietr.dftools.algorithm.model.sdf.SDFGraph">
            </input>
         </inputs>
         <outputs>
            <output
                  id="CySDF"
                  object="org.ietr.dftools.algorithm.model.sdf.SDFGraph">
            </output>
            <output
                  id="Info"
                  object="fi.abo.preesm.dataparallel.pojo.RetimingInfo">
            </output>
         </outputs>
      </task>

      <!-- Duplicate the task with old ID for compatibility -->
      <task
            id="org.abo.preesm.plugin.dataparallel.DataParallel"
            name="Data-Parallel"
            type="fi.abo.preesm.dataparallel.DataParallel">
         <inputs>
            <input
                  id="SDF"
                  object="org.ietr.dftools.algorithm.model.sdf.SDFGraph">
            </input>
         </inputs>
         <outputs>
            <output
                  id="CySDF"
                  object="org.ietr.dftools.algorithm.model.sdf.SDFGraph">
            </output>
            <output
                  id="Info"
                  object="fi.abo.preesm.dataparallel.pojo.RetimingInfo">
            </output>
         </outputs>
      </task>
   </extension>

</plugin>
